{
  "name": "vepa-admin",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "description": "open source management system",
  "author": {
    "name": "Jason Chen",
    "url": "https://github.com/jcyicai"
  },
  "license": "MIT",
  "homepage": "https://github.com/jcyicai/vue-element-plus-admin",
  "repository": {
    "type": "git",
    "url": "git@github.com:jcyicai/vue-element-plus-admin.git"
  },
  "bugs": {
    "url": "https://github.com/jcyicai/vue-element-plus-admin/issues"
  },
  "scripts": {
    "dev": "vite",
    "serve": "vite",
    "build:dev": "vue-tsc && vite build --mode development",
    "build:prod": "vue-tsc && vite build --mode production",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "preview": "npm run build:dev && vite preview",
    "prepare": "husky install",
    "lint:eslint": "eslint --fix --ext .js,.ts,.vue ./src",
    "lint:prettier": "prettier --write \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:lint-staged": "lint-staged"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vueuse/core": "^13.0.0",
    "axios": "^1.8.4",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "echarts-liquidfill": "^3.1.0",
    "element-plus": "^2.9.7",
    "md5": "^2.3.0",
    "mitt": "^3.0.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "qs": "^6.14.0",
    "screenfull": "^6.0.2",
    "sortablejs": "^1.15.6",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@rushstack/eslint-patch": "^1.11.0",
    "@tsconfig/node18": "^18.2.4",
    "@types/md5": "^2.3.5",
    "@types/node": "^22.13.12",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.18",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.20.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "sass": "^1.86.0",
    "typescript": "^5.8.2",
    "vite": "^6.0.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^2.2.8"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "vue-demi"
    ]
  }
}
